#include ".Xresources.d/colors/moria"

#define FONT_SIZE	14

!
! URxvt
!
URxvt*termName:		xterm-256color
URxvt*scrollBar:	false
URxvt*fading:		15

! Fonts
URxvt*font:		[codeset=USASCII]xft:DejaVu Sans Mono:pixelsize=FONT_SIZE,\
			xft:IPAGothic:pixelsize=FONT_SIZE,\
			xft:DejaVu Sans Mono:pixelsize=FONT_SIZE,\
			xft:Symbola:pixelsize=FONT_SIZE
URxvt*imFont:		-*-*-*-R-*-*-FONT_SIZE\-*-*-*-*-*-*
URxvt*lineSpace:	2
URxvt*letterSpace:	-1

! Extensions
URxvt*perl-ext-common:	default,selection-to-clipboard,resize-font,url-select

URxvt.url-select.button:    1
URxvt.url-select.launcher:  url-launcher
URxvt.url-select.underline: true

URxvt*keysym.C-apostrophe: perl:url-select:select_next
