#include ".Xresources.d/colors/moria"

!
! Xft
!
Xft.antialias:		1
Xft.autohint:		0
Xft.hinting:		0
Xft.hintstyle:		hintnone
! These should be set by system Xresources.
!Xft.dpi:		96
!Xft.lcdfilter:		lcddefault
!Xft.rgba:		rgb


!
! URxvt
!
URxvt*termName:		xterm-256color
URxvt*scrollBar:	false
URxvt*fading:		15

! Fonts
URxvt*font:		[codeset=USASCII]xft:DejaVu Sans Mono:pixelsize=14, \
                        xft:IPAGothic, xft:DejaVu Sans Mono, xft:Symbola
URxvt*imFont:		-*-*-*-R-*-*-14-*-*-*-*-*-*
URxvt*lineSpace:	2
URxvt*letterSpace:	-1

! Extensions
URxvt*perl-ext-common:	default,clipboard,matcher,selection-to-clipboard
URxvt*matcher.button:	1
URxvt*url-launcher:	url-launcher
