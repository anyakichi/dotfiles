"
" .vimperatorrc
"	vimperator configuration
"

set toolbars=none toolbars=tabs
set noautocomplete
set newtab=all
set focuscontent
set editor='gvim -f'
set hintchars=asdfgqwertzxcvb

set nextpattern^=^次
set previouspattern^=^前

" remove all mappings that begin with z
js mappings.getCandidates(modes.NORMAL, 'z').map(function(it) (it.names = it.names.filter(function (n) !/^z/.test(n))))

noremap a <C-p>
noremap s <C-n>
noremap z H
noremap x L

noremap <C-j> <Nop>
inoremap <C-j> <Nop>
cnoremap <C-j> <Nop>

highlight Hint	font: 12px monospace; color: black; background-color: #dddddd; border: solid 1px #bbbbbb; padding: 0px 1px 0px 1px; opacity: 0.85; border-radius: 30%;
