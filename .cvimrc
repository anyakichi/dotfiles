set noautofocus

let barposition = "bottom"

map a previousTab
map s nextTab
map <C-p> previousTab
map <C-n> nextTab

map z goBack
map x goForward

map d closeTab
map u lastClosedTab

map <C-f> scrollFullPageDown
map <C-b> scrollFullPageUp

map gr :source settings.configpath<CR>

map R reloadTabUncached

map gz centerMatchH
map gt centerMatchT
map gb centerMatchB

imap <C-h> deleteChar
imap <C-o> editWithVim

let homedirectory = "/home/inajima"
let configpath = "~/.cvimrc"
set localconfig
